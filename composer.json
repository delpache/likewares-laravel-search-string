{
    "name": "likewares/laravel-search-string",
    "type": "library",
    "description": "Génère des requêtes de base de données basées sur une chaîne unique à l'aide d'une syntaxe simple et personnalisable.",
    "license": "MIT",
    "authors": [
        {
            "name": "KOUAOBILE ASSANDE HUGUES",
            "email": "assandehugues@gmail.com",
            "homepage": "https://likewares.com",
            "role": "CEO Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3|^8.4",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "hoa/compiler": "^3.17",
        "sanmai/hoa-protocol": "^1.17"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0|^12.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10"
    },
    "autoload": {
       "psr-4": {
            "Likewares\\LaravelSearchString\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Likewares\\LaravelSearchString\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Likewares\\LaravelSearchString\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
